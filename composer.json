{
    "name": "supercool/shopify",
    "description": "load your shopify-products into your craft3 cms instance",
    "type": "craft-plugin",
    "version": "2.2.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "shopify",
        "fetch products",
        "integrate shopify",
        "product sync",
        "ecommerce"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "niklas",
            "homepage": "https://maier-niklas.de/"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0",
        "guzzlehttp/guzzle": "^6.3",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "shopify\\": "src/"
        }
    },
    "extra": {
        "name": "shopify",
        "handle": "shopify",
        "class": "shopify\\Shopify"
    }
}
